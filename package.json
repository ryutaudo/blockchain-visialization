{
  "name": "my-awesome-package",
  "version": "1.0.0",
  "description": "The best package you will ever find.",
  "main": "index.js",
  "repository": {
    "url": "https://github.com/yarnpkg/example-yarn-package",
    "type": "git"
  },
  "author": "Yarn Contributor",
  "license": "MIT",
  "scripts": {
    "start": "babel-node server/index.js --presets es2015,stage-3",
    "hack": "webpack && webpack-dev-server",
    "build": "webpack",
    "postinstall": "yarn build && yarn migrate && yarn seed",
    "migrate": "knex migrate:latest --knexfile server/db/knexfile.js",
    "rollback": "knex migrate:rollback --knexfile server/db/knexfile.js",
    "seed": "knex seed:run --knexfile server/db/knexfile.js"
  },
  "dependencies": {
    "babel-preset-stage-3": "^6.24.1",
    "body-parser": "^1.18.2",
    "d3": "^4.13.0",
    "express": "^4.16.2",
    "knex": "^0.14.3",
    "morgan": "^1.9.0",
    "pg": "^7.4.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-scripts": "^1.1.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "open-browser-webpack-plugin": "^0.0.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.9",
    "es2015": "^0.0.0",
    "source-map": "^0.7.0",
    "style-loader": "^0.20.1",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "~7.6.1",
    "jest": "^22.1.4",
    "json-loader": "^0.5.4",
    "react-addons-test-utils": "^15.6.2",
    "sinon": "^4.2.2"
  }
}
